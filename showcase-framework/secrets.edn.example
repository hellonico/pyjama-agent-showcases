;; Example secrets.edn configuration
;; Copy this to pyjama/secrets.edn and add your actual API keys

{;; TMDB API Key (required for movie review agent)
 ;; Get yours at: https://www.themoviedb.org/settings/api
 :tmdb-api-key "YOUR_TMDB_API_KEY_HERE"
 
 ;; LLM Configuration (choose one)
 ;; OpenAI
 :openai-api-key "YOUR_OPENAI_KEY"
 
 ;; Anthropic Claude
 :anthropic-api-key "YOUR_ANTHROPIC_KEY"
 
 ;; Google Gemini
 :google-api-key "YOUR_GEMINI_KEY"
 
 ;; Or configure in ~/.pyjama/config.edn for global settings
 }
