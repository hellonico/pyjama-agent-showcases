{:paths ["src" "resources"]

 :deps {org.clojure/clojure {:mvn/version "1.11.1"}

        ;; Common showcase framework (provides Pyjama, Ring, Jetty, Reagent, AJAX, etc.)
        hellonico/showcase-framework {:local/root "../showcase-framework"}

        ;; Showcase-specific dependencies
        ;; HTTP Client for TMDB API
        clj-http/clj-http {:mvn/version "3.12.3"}
        cheshire/cheshire {:mvn/version "5.11.0"}}

 :aliases {:server {:main-opts ["-m" "movie-review.server"]}
           :cli {:main-opts ["-m" "showcase.cli"]}}}
