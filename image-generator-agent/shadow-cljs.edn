{:source-paths ["src" "../showcase-framework/src"]
 :dependencies [[reagent "1.2.0"]      ; ClojureScript deps needed for compilation
                [cljs-ajax "0.8.4"]]   ; (Backend deps come from deps.edn)
 :builds {:app {:target :browser
                :output-dir "public/js"
                :asset-path "/js"
                :modules {:main {:init-fn image-generator.core/init!}}
                :devtools {:http-root "public"
                           :http-port 8020
                           :after-load image-generator.core/reload!}}}}
